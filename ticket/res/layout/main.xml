<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    >
<Button android:text="@string/buyButtonText" android:layout_height="wrap_content" android:id="@+id/buy" android:layout_width="match_parent" android:gravity="center|fill_vertical" android:clickable="true" android:soundEffectsEnabled="true"></Button>
<TextView android:layout_height="wrap_content" android:id="@+id/TextView01" android:layout_width="fill_parent" android:text="@string/hello"></TextView>





<GridView android:id="@+id/GridView01" android:layout_width="wrap_content" android:layout_height="wrap_content"></GridView>
<TimePicker android:id="@+id/TimePicker01" android:layout_width="wrap_content" android:layout_height="wrap_content"></TimePicker>



<VideoView android:id="@+id/VideoView01" android:layout_width="wrap_content" android:layout_height="wrap_content"></VideoView>
</LinearLayout>
